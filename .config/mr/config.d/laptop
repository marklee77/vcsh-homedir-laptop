[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/laptop.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-laptop.git laptop
setpush  = vcsh laptop remote set-url --push origin git@stillwell.me:mark/vcsh-homedir-laptop.git
           vcsh laptop remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-laptop.git
           vcsh laptop remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-laptop.git

